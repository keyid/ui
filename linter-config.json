{
    "Linters": {
        "errchk":   "errchk -blank -asserts -ignoretests -abspath {path}:^(?P<path>.*?\\.go):(?P<line>\\d+):(?P<col>\\d+):(warning|error):\\s*(?P<message>.*)$",
        "gas":      "gas -rule=sql -rule=unsafe -rule=perms -rule=templates -rule=rand -rule=hardcoded -rule=tls_old -rule=exec -rule=httpoxy -rule=crypto -rule=tempfile -rule=tls_good -rule=tls_ok -rule=bind -rule=rsa -fmt=csv {path}/*.go:^(?P<path>.*?\\.go),(?P<line>\\d+),(?P<message>[^,]+)",
        "misspell": "misspell -j 1 -locale US {path}/*.go:PATH:LINE:COL:MESSAGE"
    },
    "DisableAll": true,
    "Enable": [
	    "aligncheck",
        "deadcode",
        "errchk",
        "gas",
        "gofmt",
        "goimports",
        "golint",
        "gosimple",
        "ineffassign",
        "interfacer",
        "misspell",
        "staticcheck",
        "structcheck",
        "unconvert",
        "vet",
        "vetshadow"
    ],
    "Severity": {
        "aligncheck": "warning",
        "deadcode": "error",
        "errchk": "error",
        "gas": "error",
        "gofmt": "error",
        "goimports": "error",
        "golint": "error",
        "gosimple": "error",
        "ineffassign": "error",
        "interfacer": "error",
        "misspell": "error",
        "staticcheck": "error",
        "structcheck": "warning",
        "unconvert": "error",
        "vet": "error",
        "vetshadow": "error"
    },
    "Vendor": true,
    "Test": true,
    "Cyclo": 15,
    "Concurrency": 3,
    "EnableGC": false,
    "Deadline": "120m"
}